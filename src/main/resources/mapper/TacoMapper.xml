<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zimo.taco.mapper.TacoMapper">

    <insert id="addTaco" keyProperty="id" useGeneratedKeys="true">
        INSERT INTO Taco (name, ingredients)
        VALUES (#{name}, #{ingredients})
    </insert>

</mapper>